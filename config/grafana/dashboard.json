    {
      "id": 1,
      "title": "Test Status",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 0, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "prometheus" },
          "expr": "rtp_test_state{test_id=~\"$test_id\"}",
          "legendFormat": "State",
          "refId": "A"
        }
      ],
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "center",
        "orientation": "auto",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "textMode": "value_and_name"
      },
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": { "0": { "text": "CREATED", "color": "blue" } }
            },
            {
              "type": "value",
              "options": { "1": { "text": "RUNNING", "color": "green" } }
            },
            {
              "type": "value",
              "options": { "2": { "text": "STOPPED", "color": "orange" } }
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "value": null, "color": "blue" },
              { "value": 1, "color": "green" },
              { "value": 2, "color": "orange" }
            ]
          }
        }
      }
    },
